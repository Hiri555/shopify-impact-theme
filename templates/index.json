    "custom_liquid_tnMJLj": {
      "type": "custom-liquid",
      "name": "Custom Liquid",
      "settings": {
        "full_width": true,
        "remove_vertical_spacing": false,
        "remove_horizontal_spacing": false,
        "liquid": "<style>\n  :root {\n    --background-color: #fffefd;\n    --text-color: #484848;\n    --accent-color: #F1E5DD;\n    --accent-light: #F1E5DD;\n    --card-bg: #fffefd;\n    --image-bg: #F1E5DD;\n    --font-body: system-ui, -apple-system, 'Segoe UI', Roboto, 'Helvetica Neue', sans-serif;\n    --font-accent: 'Instrument Serif', Georgia, serif;\n    --card-border-radius: 12px;\n    --number-size: 40px;\n  }\n\n  .steps-section {\n    max-width: 1200px;\n    margin: 0 auto;\n    padding: 60px 20px;\n  }\n\n  .section-title {\n    text-align: center;\n    font-size: 34px;\n    font-weight: 400;\n    color: var(--text-color);\n    margin-bottom: 40px;\n    letter-spacing: -0.5px;\n  }\n\n  .accent-text {\n    display: block;\n    font-family: var(--font-accent);\n    font-style: italic;\n    font-weight: 300;\n    color: #ff9eb2;\n    text-transform: lowercase;\n    font-size: 0.95em;\n    margin-top: 4px;\n  }\n\n  .steps-container-desktop {\n    display: none;\n  }\n\n  @media (min-width: 769px) {\n    .steps-container-desktop {\n      display: grid;\n      grid-template-columns: repeat(4, 1fr);\n      gap: 20px;\n    }\n\n    .steps-container-mobile {\n      display: none;\n    }\n  }\n\n  .step-card {\n    background: var(--card-bg);\n    border: 1px dashed var(--accent-color);\n    border-radius: var(--card-border-radius);\n    overflow: hidden;\n  }\n\n  .step-image-container {\n    background-color: var(--image-bg);\n    aspect-ratio: 1 / 1.1;\n    overflow: hidden;\n  }\n\n  .step-image {\n    width: 100%;\n    height: 100%;\n    object-fit: cover;\n  }\n\n  .step-content {\n    padding: 20px 16px;\n    background: var(--card-bg);\n  }\n\n  .step-title-container {\n    display: flex;\n    align-items: center;\n    gap: 12px;\n    margin-bottom: 12px;\n  }\n\n  .number-icon {\n    width: var(--number-size);\n    height: var(--number-size);\n    min-width: var(--number-size);\n    border: 2px solid #ff9eb2;\n    border-radius: 50%;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    font-weight: 600;\n    font-size: 18px;\n    color: var(--text-color);\n    background: var(--card-bg);\n  }\n\n  .step-title {\n    font-size: 18px;\n    font-weight: 500;\n    color: var(--text-color);\n  }\n\n  .step-description {\n    font-size: 13px;\n    color: var(--text-color);\n    line-height: 1.5;\n    font-weight: 400;\n  }\n\n  @media (max-width: 768px) {\n    .section-title {\n      font-size: 24px;\n      margin-bottom: 30px;\n      padding: 0 15px;\n    }\n\n    .steps-container-mobile {\n      position: relative;\n      overflow: hidden;\n      padding: 0 15px;\n    }\n\n    .carousel-track {\n      display: flex;\n      transition: transform 0.4s ease;\n    }\n\n    .carousel-slide {\n      min-width: 100%;\n      padding: 0;\n    }\n\n    .step-card {\n      max-width: 100%;\n      margin: 0 auto;\n    }\n\n    .step-image-container {\n      aspect-ratio: 1 / 1.05;\n    }\n\n    .step-content {\n      padding: 20px 16px 24px;\n    }\n\n    .number-icon {\n      width: 50px;\n      height: 50px;\n      min-width: 50px;\n      font-size: 20px;\n    }\n\n    .step-title {\n      font-size: 20px;\n      font-weight: 600;\n    }\n\n    .step-description {\n      font-size: 14px;\n      line-height: 1.6;\n    }\n\n    .carousel-navigation {\n      display: flex;\n      justify-content: center;\n      align-items: center;\n      gap: 16px;\n      margin-top: 30px;\n    }\n\n    .nav-button {\n      width: 56px;\n      height: 56px;\n      border-radius: 50%;\n      border: none;\n      display: flex;\n      align-items: center;\n      justify-content: center;\n      cursor: pointer;\n      transition: all 0.2s ease;\n    }\n\n    .nav-button.prev {\n      background: var(--card-bg);\n      color: var(--text-color);\n      border: 1px solid #f0f0f0;\n    }\n\n    .nav-button.next {\n      background: #ff9eb2;\n      color: var(--card-bg);\n    }\n\n    .nav-button:disabled {\n      opacity: 0.5;\n      cursor: not-allowed;\n    }\n\n    .nav-button svg {\n      width: 24px;\n      height: 24px;\n    }\n  }\n\n  @media (min-width: 769px) {\n    .carousel-navigation {\n      display: none;\n    }\n  }\n</style>\n\n<section class=\"steps-section\">\n  <h2 class=\"section-title\">\n    Comment utiliser le\n    <span class=\"accent-text\">fond de teint adaptatif</span>\n  </h2>\n\n  <div class=\"steps-container-desktop\">\n    <div class=\"step-card\">\n      <div class=\"step-image-container\">\n        <img class=\"step-image\" src=\"https://myeclera.com/cdn/shop/files/1_f219f9ba-b888-4423-ae19-8b07f7c7580c.webp?v=1768361404&width=618\" alt=\"Étape 1\">\n      </div>\n      <div class=\"step-content\">\n        <div class=\"step-title-container\">\n          <div class=\"number-icon\">1</div>\n          <h3 class=\"step-title\">Étape 1</h3>\n        </div>\n        <p class=\"step-description\">Appliquez la base Eclera pour lisser la peau et la préparer.</p>\n      </div>\n    </div>\n\n    <div class=\"step-card\">\n      <div class=\"step-image-container\">\n        <img class=\"step-image\" src=\"https://myeclera.com/cdn/shop/files/2_d494b6ea-0db8-4fcf-b04e-ea778a2c169e.webp?v=1768361403&width=618\" alt=\"Étape 2\">\n      </div>\n      <div class=\"step-content\">\n        <div class=\"step-title-container\">\n          <div class=\"number-icon\">2</div>\n          <h3 class=\"step-title\">Étape 2</h3>\n        </div>\n        <p class=\"step-description\">Déposez une petite quantité de Fond de teint 4-en-1 sur le dos de la main.</p>\n      </div>\n    </div>\n\n    <div class=\"step-card\">\n      <div class=\"step-image-container\">\n        <img class=\"step-image\" src=\"https://myeclera.com/cdn/shop/files/3_5cf6b63f-d294-40eb-a10c-472428dbb4a8.webp?v=1768361404&width=618\" alt=\"Étape 3\">\n      </div>\n      <div class=\"step-content\">\n        <div class=\"step-title-container\">\n          <div class=\"number-icon\">3</div>\n          <h3 class=\"step-title\">Étape 3</h3>\n        </div>\n        <p class=\"step-description\">Appliquez du centre du visage vers l'extérieur, à l'aide d'un pinceau ou des doigts.</p>\n      </div>\n    </div>\n\n    <div class=\"step-card\">\n      <div class=\"step-image-container\">\n        <img class=\"step-image\" src=\"https://myeclera.com/cdn/shop/files/4.gif?v=1768361411&width=618\" alt=\"Étape 4\">\n      </div>\n      <div class=\"step-content\">\n        <div class=\"step-title-container\">\n          <div class=\"number-icon\">4</div>\n          <h3 class=\"step-title\">Étape 4</h3>\n        </div>\n        <p class=\"step-description\">Utilisez une fine couche pour un rendu naturel, puis ajoutez une couche sur les zones à couvrir si nécessaire.</p>\n      </div>\n    </div>\n  </div>\n\n  <div class=\"steps-container-mobile\">\n    <div class=\"carousel-track\" id=\"carouselTrack\">\n      <div class=\"carousel-slide\">\n        <div class=\"step-card\">\n          <div class=\"step-image-container\">\n            <img class=\"step-image\" src=\"https://myeclera.com/cdn/shop/files/1_f219f9ba-b888-4423-ae19-8b07f7c7580c.webp?v=1768361404&width=618\" alt=\"Étape 1\">\n          </div>\n          <div class=\"step-content\">\n            <div class=\"step-title-container\">\n              <div class=\"number-icon\">1</div>\n              <h3 class=\"step-title\">Étape 1</h3>\n            </div>\n            <p class=\"step-description\">Appliquez la base Eclera pour lisser la peau et la préparer.</p>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"carousel-slide\">\n        <div class=\"step-card\">\n          <div class=\"step-image-container\">\n            <img class=\"step-image\" src=\"https://myeclera.com/cdn/shop/files/2_d494b6ea-0db8-4fcf-b04e-ea778a2c169e.webp?v=1768361403&width=618\" alt=\"Étape 2\">\n          </div>\n          <div class=\"step-content\">\n            <div class=\"step-title-container\">\n              <div class=\"number-icon\">2</div>\n              <h3 class=\"step-title\">Étape 2</h3>\n            </div>\n            <p class=\"step-description\">Déposez une petite quantité de Fond de teint 4-en-1 sur le dos de la main.</p>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"carousel-slide\">\n        <div class=\"step-card\">\n          <div class=\"step-image-container\">\n            <img class=\"step-image\" src=\"https://myeclera.com/cdn/shop/files/3_5cf6b63f-d294-40eb-a10c-472428dbb4a8.webp?v=1768361404&width=618\" alt=\"Étape 3\">\n          </div>\n          <div class=\"step-content\">\n            <div class=\"step-title-container\">\n              <div class=\"number-icon\">3</div>\n              <h3 class=\"step-title\">Étape 3</h3>\n            </div>\n            <p class=\"step-description\">Appliquez du centre du visage vers l'extérieur, à l'aide d'un pinceau ou des doigts.</p>\n          </div>\n        </div>\n      </div>\n\n      <div class=\"carousel-slide\">\n        <div class=\"step-card\">\n          <div class=\"step-image-container\">\n            <img class=\"step-image\" src=\"https://myeclera.com/cdn/shop/files/4.gif?v=1768361411&width=618\" alt=\"Étape 4\">\n          </div>\n          <div class=\"step-content\">\n            <div class=\"step-title-container\">\n              <div class=\"number-icon\">4</div>\n              <h3 class=\"step-title\">Étape 4</h3>\n            </div>\n            <p class=\"step-description\">Utilisez une fine couche pour un rendu naturel, puis ajoutez une couche sur les zones à couvrir si nécessaire.</p>\n          </div>\n        </div>\n      </div>\n    </div>\n\n    <div class=\"carousel-navigation\">\n      <button class=\"nav-button prev\" id=\"prevBtn\" disabled>\n        <svg xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\" stroke-width=\"2\">\n          <polyline points=\"15 18 9 12 15 6\"></polyline>\n        </svg>\n      </button>\n      <button class=\"nav-button next\" id=\"nextBtn\">\n        <svg xmlns=\"http://www.w3.org/2000/svg\" fill=\"none\" viewBox=\"0 0 24 24\" stroke=\"currentColor\" stroke-width=\"2\">\n          <polyline points=\"9 18 15 12 9 6\"></polyline>\n        </svg>\n      </button>\n    </div>\n  </div>\n</section>\n\n<script>\n  const track = document.getElementById('carouselTrack');\n  const prevBtn = document.getElementById('prevBtn');\n  const nextBtn = document.getElementById('nextBtn');\n  const totalSlides = 4;\n  let currentSlide = 0;\n\n  function updateCarousel() {\n    track.style.transform = `translateX(-${currentSlide * 100}%)`;\n    prevBtn.disabled = currentSlide === 0;\n    nextBtn.disabled = currentSlide === totalSlides - 1;\n    \n    if (currentSlide === 0) {\n      prevBtn.style.opacity = '0.5';\n    } else {\n      prevBtn.style.opacity = '1';\n    }\n    \n    if (currentSlide === totalSlides - 1) {\n      nextBtn.style.opacity = '0.5';\n    } else {\n      nextBtn.style.opacity = '1';\n    }\n  }\n\n  prevBtn.addEventListener('click', () => {\n    if (currentSlide > 0) {\n      currentSlide--;\n      updateCarousel();\n    }\n  });\n\n  nextBtn.addEventListener('click', () => {\n    if (currentSlide < totalSlides - 1) {\n      currentSlide++;\n      updateCarousel();\n    }\n  });\n\n  let touchStartX = 0;\n  let touchEndX = 0;\n\n  track.addEventListener('touchstart', (e) => {\n    touchStartX = e.changedTouches[0].screenX;\n  }, { passive: true });\n\n  track.addEventListener('touchend', (e) => {\n    touchEndX = e.changedTouches[0].screenX;\n    handleSwipe();\n  }, { passive: true });\n\n  function handleSwipe() {\n    const swipeThreshold = 50;\n    const diff = touchStartX - touchEndX;\n    \n    if (diff > swipeThreshold && currentSlide < totalSlides - 1) {\n      currentSlide++;\n      updateCarousel();\n    } else if (diff < -swipeThreshold && currentSlide > 0) {\n      currentSlide--;\n      updateCarousel();\n    }\n  }\n\n  updateCarousel();\n</script>",
